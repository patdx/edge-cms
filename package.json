{
  "name": "edge-cms",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "rakkas",
    "build": "rakkas build",
    "start": "node dist/server",
    "start-miniflare": "miniflare --modules dist/server/cloudflare-workers-bundle.js",
    "format": "prettier --write --ignore-unknown src",
    "test": "npm run test:typecheck && npm run test:format && npm run test:lint && npm run test:unit",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:format": "prettier --check --ignore-unknown src",
    "test:lint": "eslint . --ignore-pattern dist",
    "test:unit": "vitest run",
    "preinstall": "npx only-allow pnpm",
    "pages-deploy": "wrangler pages publish dist/client/ --project-name edge-cms",
    "pages-preview": "wrangler pages dev dist/client/"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20240329.0",
    "@hattip/adapter-cloudflare-workers": "0.0.45",
    "@hattip/bundler-cloudflare-workers": "0.0.45",
    "@ltd/j-toml": "1.38.0",
    "@miniflare/d1": "2.14.2",
    "@miniflare/shared": "2.14.2",
    "@miniflare/tre": "3.0.0-next.14",
    "@rakkasjs/eslint-config": "0.6.20",
    "@tailwindcss/forms": "0.5.7",
    "@types/better-sqlite3": "7.6.9",
    "@types/gensync": "1.0.4",
    "@types/lodash-es": "4.17.12",
    "@types/react": "18.2.73",
    "@types/react-dom": "18.2.23",
    "autoprefixer": "10.4.19",
    "better-sqlite3": "9.4.3",
    "daisyui": "4.9.0",
    "eslint": "8.57.0",
    "miniflare": "3.20240320.1",
    "only-allow": "^1.1.1",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "rakkasjs": "0.6.20",
    "sql-formatter": "15.3.0",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.3",
    "vite": "4.5.3",
    "vite-node": "1.4.0",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.4.0",
    "wrangler": "3.41.0"
  },
  "dependencies": {
    "@hattip/response": "0.0.45",
    "@monaco-editor/react": "^4.5.1",
    "@rjsf/core": "5.18.1",
    "@rjsf/utils": "5.18.1",
    "@rjsf/validator-ajv8": "5.18.1",
    "@sinclair/typebox": "0.32.20",
    "@types/json-schema": "7.0.15",
    "clsx": "2.1.0",
    "dirty-json": "0.9.2",
    "gensync": "1.0.0-beta.2",
    "lodash-es": "4.17.21",
    "monaco-editor": "^0.47.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.13",
    "sql-template-tag": "5.2.0",
    "sqlstring-sqlite": "0.1.1",
    "zod": "3.22.4",
    "zustand": "4.5.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ]
  },
  "packageManager": "pnpm@8.15.5"
}
