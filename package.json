{
  "name": "edge-cms",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "rakkas",
    "build": "rakkas build",
    "start": "node dist/server",
    "start-miniflare": "miniflare --modules dist/server/cloudflare-workers-bundle.js",
    "format": "prettier --write --ignore-unknown src",
    "test": "npm run test:typecheck && npm run test:format && npm run test:lint && npm run test:unit",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:format": "prettier --check --ignore-unknown src",
    "test:lint": "eslint . --ignore-pattern dist",
    "test:unit": "vitest run",
    "preinstall": "npx only-allow pnpm",
    "pages-deploy": "wrangler pages publish dist/client/ --project-name edge-cms",
    "pages-preview": "wrangler pages dev dist/client/"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20230922.0",
    "@hattip/adapter-cloudflare-workers": "0.0.35",
    "@hattip/bundler-cloudflare-workers": "0.0.35",
    "@ltd/j-toml": "1.38.0",
    "@miniflare/d1": "2.14.1",
    "@miniflare/shared": "2.14.1",
    "@miniflare/tre": "3.0.0-next.13",
    "@rakkasjs/eslint-config": "0.6.20",
    "@tailwindcss/forms": "0.5.6",
    "@types/better-sqlite3": "7.6.5",
    "@types/gensync": "1.0.1",
    "@types/lodash-es": "4.17.9",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.8",
    "autoprefixer": "10.4.16",
    "better-sqlite3": "8.7.0",
    "daisyui": "3.8.2",
    "eslint": "8.50.0",
    "miniflare": "3.20230922.0",
    "only-allow": "^1.1.1",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "rakkasjs": "0.6.20",
    "sql-formatter": "13.0.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-node": "0.34.6",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6",
    "wrangler": "3.10.1"
  },
  "dependencies": {
    "@hattip/response": "0.0.35",
    "@monaco-editor/react": "^4.5.1",
    "@rjsf/core": "5.13.0",
    "@rjsf/utils": "5.13.0",
    "@rjsf/validator-ajv8": "5.13.0",
    "@sinclair/typebox": "0.31.17",
    "@types/json-schema": "7.0.13",
    "clsx": "2.0.0",
    "dirty-json": "0.9.2",
    "gensync": "1.0.0-beta.2",
    "lodash-es": "4.17.21",
    "monaco-editor": "^0.43.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "sql-template-tag": "5.1.0",
    "sqlstring-sqlite": "0.1.1",
    "zod": "3.22.3",
    "zustand": "4.4.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ]
  },
  "packageManager": "pnpm@8.8.0"
}
