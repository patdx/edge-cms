{
  "name": "edge-cms",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "rakkas",
    "build": "rakkas build",
    "start": "node dist/server",
    "start-miniflare": "miniflare --modules dist/server/cloudflare-workers-bundle.js",
    "format": "prettier --write --ignore-unknown src",
    "test": "npm run test:typecheck && npm run test:format && npm run test:lint && npm run test:unit",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:format": "prettier --check --ignore-unknown src",
    "test:lint": "eslint . --ignore-pattern dist",
    "test:unit": "vitest run",
    "preinstall": "npx only-allow pnpm",
    "pages-deploy": "wrangler pages publish dist/client/ --project-name edge-cms",
    "pages-preview": "wrangler pages dev dist/client/"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20230404.0",
    "@hattip/adapter-cloudflare-workers": "0.0.33",
    "@hattip/bundler-cloudflare-workers": "0.0.33",
    "@ltd/j-toml": "1.38.0",
    "@miniflare/d1": "2.13.0",
    "@miniflare/shared": "2.13.0",
    "@miniflare/tre": "3.0.0-next.12",
    "@rakkasjs/eslint-config": "0.6.20",
    "@tailwindcss/forms": "0.5.3",
    "@types/better-sqlite3": "7.6.4",
    "@types/gensync": "1.0.0",
    "@types/lodash-es": "4.17.7",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "better-sqlite3": "8.3.0",
    "daisyui": "2.51.5",
    "eslint": "8.38.0",
    "miniflare": "2.13.0",
    "only-allow": "^1.1.1",
    "postcss": "8.4.21",
    "prettier": "2.8.7",
    "rakkasjs": "0.6.20",
    "sql-formatter": "12.2.0",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4",
    "vite": "4.2.1",
    "vite-node": "0.30.1",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.30.1",
    "wrangler": "2.15.1"
  },
  "dependencies": {
    "@hattip/response": "0.0.33",
    "@monaco-editor/react": "^4.5.0",
    "@rjsf/core": "5.6.0",
    "@rjsf/utils": "5.6.0",
    "@rjsf/validator-ajv8": "5.6.0",
    "@sinclair/typebox": "0.27.8",
    "@types/json-schema": "7.0.11",
    "clsx": "1.2.1",
    "dirty-json": "0.9.2",
    "gensync": "1.0.0-beta.2",
    "lodash-es": "4.17.21",
    "monaco-editor": "^0.37.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.3",
    "sql-template-tag": "5.0.3",
    "sqlstring-sqlite": "0.1.1",
    "zod": "3.21.4",
    "zustand": "4.3.7"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ]
  }
}
